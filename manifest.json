{
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "name": "Discord Button",
  "version": "1.0.0",
  "permissions": ["activeTab", "tabs", "scripting"],
  "host_permissions": ["*://*/*"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js", "axios.min.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
